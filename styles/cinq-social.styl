@font-face {
	font-family: 'Cinq-social';
	src: url('webfonts/Cinq-social.eot?#') format('eot'),
		url('webfonts/Cinq-social.svg#Cinq-social') format('svg'),
		url('webfonts/Cinq-social.woff') format('woff'),
		url('webfonts/Cinq-social.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

/* Use the following CSS code if you want to use data attributes for inserting your icons */
/*[data-icon]:before {
	font-family: 'Cinq-social';
	content: attr(data-icon);
	speak: none;
}*/

/* Use the following CSS code if you want to have a class per icon */
[class^="icon-"]:before, [class*=" icon-"]:before {
	font-family: 'Cinq-social';
	font-style: normal;
	speak: none;
}
.icon-instagram:before {
	content: "\e000";
	font-size: 1.1em;
	position: relative;
	top: 1px;
}
